            <div class="user-card">
              <p><%=user.username%></p>
              <p><%=user.name%></p>
            </div>
          <% } %>
        </div>
      </div>
      <div id="onlyInFollowingDiv" class="outer-content-container">
        <div class="content-top-div">
          <h3>Only In Following</h3>
          <a role="button" class="download-button" href="/download/onlyInFollowing.json" aria-label="Download the only in following data in JSON format" download>
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-download" viewBox="0 0 16 16">
              <path d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z"/>
              <path d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z"/>
            </svg>
          </a>
        </div>
        <div id="onlyInFollowing" class="content-container">
          <% for (const user of data[4]) {%>
            <div class="user-card">
              <p><%=user.username%></p>
              <p><%=user.name%></p>
            </div>
          <% } %>
        </div>
      </div>
    </div>
  </div>
</main>
<script src="/display-results.js"></script>
<%- include('partials/footer.ejs') %>