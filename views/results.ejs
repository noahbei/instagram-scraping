<%- include('partials/header.ejs') %>
<main id="results-main" class="py-4">
  <div class="all-container">
    <div id="input-container" class="grid-container">
      <div class="outer-content-container">
        <div class="content-top-div">
          <h3>Followers</h3>
          <a role="button" class="download-button" href="/download/followers.json" aria-label="Download the followers data in JSON format" download>
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-download" viewBox="0 0 16 16">
              <path d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z"/>
              <path d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z"/>
            </svg>
          </a>
        </div>
        <div id="followers" class="content-container">
          <% for (const user of data[0]) {%>
            <div class="user-card">
              <p><%=user.username%></p>
              <p><%=user.name%></p>
              <a href="https://instagram.com/<%= user.username %>/">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-box-arrow-up-right" viewBox="0 0 16 16">
                  <path fill-rule="evenodd" d="M8.636 3.5a.5.5 0 0 0-.5-.5H1.5A1.5 1.5 0 0 0 0 4.5v10A1.5 1.5 0 0 0 1.5 16h10a1.5 1.5 0 0 0 1.5-1.5V7.864a.5.5 0 0 0-1 0V14.5a.5.5 0 0 1-.5.5h-10a.5.5 0 0 1-.5-.5v-10a.5.5 0 0 1 .5-.5h6.636a.5.5 0 0 0 .5-.5z"/>
                  <path fill-rule="evenodd" d="M16 .5a.5.5 0 0 0-.5-.5h-5a.5.5 0 0 0 0 1h3.793L6.146 9.146a.5.5 0 1 0 .708.708L15 1.707V5.5a.5.5 0 0 0 1 0v-5z"/>
                </svg>
              </a>
            </div>
          <% } %>
        </div>
      </div>
      <div class="outer-content-container">
        <div class="content-top-div">
          <h3>Following</h3>
          <a role="button" class="download-button" href="/download/following.json" aria-label="Download the following data in JSON format" download>
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-download" viewBox="0 0 16 16">
              <path d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z"/>
              <path d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z"/>
            </svg>
          </a>
        </div>
        <div id="following" class="content-container">
          <% for (const user of data[1]) {%>
            <div class="user-card">
              <p><%=user.username%></p>
              <p><%=user.name%></p>
            </div>
          <% } %>
        </div>
      </div>
    </div>
    <div id="output-container" class="grid-container">
      <div class="outer-content-container">
        <div class="content-top-div">
          <h3>Overlap</h3>
          <a role="button" class="download-button" href="/download/overlap.json" aria-label="Download the overlap data in JSON format" download>
